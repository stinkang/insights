ALWAYS_EMBED_SWIFT_STANDARD_LIBRARIES = YES
FRAMEWORK_SEARCH_PATHS = $(inherited) "${PODS_CONFIGURATION_BUILD_DIR}/Charts" "${PODS_CONFIGURATION_BUILD_DIR}/HMSegmentedControl" "${PODS_CONFIGURATION_BUILD_DIR}/MXPagerView" "${PODS_CONFIGURATION_BUILD_DIR}/MXParallaxHeader" "${PODS_CONFIGURATION_BUILD_DIR}/MXSegmentedPager" "${PODS_CONFIGURATION_BUILD_DIR}/UICountingLabel"
GCC_PREPROCESSOR_DEFINITIONS = $(inherited) COCOAPODS=1
LD_RUNPATH_SEARCH_PATHS = $(inherited) '@executable_path/Frameworks' '@loader_path/Frameworks'
OTHER_CFLAGS = $(inherited) -iquote "${PODS_CONFIGURATION_BUILD_DIR}/Charts/Charts.framework/Headers" -iquote "${PODS_CONFIGURATION_BUILD_DIR}/HMSegmentedControl/HMSegmentedControl.framework/Headers" -iquote "${PODS_CONFIGURATION_BUILD_DIR}/MXPagerView/MXPagerView.framework/Headers" -iquote "${PODS_CONFIGURATION_BUILD_DIR}/MXParallaxHeader/MXParallaxHeader.framework/Headers" -iquote "${PODS_CONFIGURATION_BUILD_DIR}/MXSegmentedPager/MXSegmentedPager.framework/Headers" -iquote "${PODS_CONFIGURATION_BUILD_DIR}/UICountingLabel/UICountingLabel.framework/Headers"
OTHER_LDFLAGS = $(inherited) -framework "Charts" -framework "HMSegmentedControl" -framework "MXPagerView" -framework "MXParallaxHeader" -framework "MXSegmentedPager" -framework "UICountingLabel"
OTHER_SWIFT_FLAGS = $(inherited) "-D" "COCOAPODS"
PODS_BUILD_DIR = ${BUILD_DIR}
PODS_CONFIGURATION_BUILD_DIR = ${PODS_BUILD_DIR}/$(CONFIGURATION)$(EFFECTIVE_PLATFORM_NAME)
PODS_PODFILE_DIR_PATH = ${SRCROOT}/.
PODS_ROOT = ${SRCROOT}/Pods
